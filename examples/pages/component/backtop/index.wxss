.backtop-container {
  height: 1000px;
}

.view-anchor {
  width: 100%;
  height: 60vh;
  margin-top: 80rpx;
  background-color: #eee;
}

.backtop-slot {
  width: 80rpx;
  line-height: 80rpx;
  color: rgb(25, 137, 250);
  text-align: center;
  background-color: rgb(242, 245, 246);
  box-shadow: rgba(0, 0, 0, 0.12) 0 0 6px;
}

.backtop-hide {
  display: none;
}
