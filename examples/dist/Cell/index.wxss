.lin-cell{background-color:#fff;padding:22rpx 32rpx;box-sizing:border-box;position:relative}.lin-cell::after{position:absolute;box-sizing:border-box;content:" ";pointer-events:none;right:32rpx;bottom:0;left:32rpx;border-bottom:0 solid #e4e7ed}.lin-cell.lin-cell-large{padding-top:26rpx;padding-bottom:26rpx}.lin-cell-border::after{border-width:1px}.lin-cell-header{display:flex;flex-direction:row;justify-content:space-between;font-size:28rpx}.lin-cell-text{display:flex;flex-direction:row;flex-shrink:0;align-items:center}.lin-cell-title{color:#323233;text-align:left;margin-right:10rpx;width:0}.lin-cell-title-icon{margin-right:10rpx}.lin-cell-value{color:#606266;text-align:right;margin-left:10rpx;flex:1}.lin-cell-value-icon{margin-left:10rpx}.lin-cell-content{color:#606266;font-size:24rpx;margin-top:10rpx}.lin-cell-required::before{content:"*";position:absolute;left:8px;color:#ee0a24;font-size:28rpx}.lin-cell-item-value{text-align:right;flex:1}