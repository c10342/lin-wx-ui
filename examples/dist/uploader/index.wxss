@import '../common/base.wxss';.lin-uploader{display:flex;flex-direction:row;flex-wrap:wrap}.lin-uploader-preview{position:relative;margin:0 16rpx 16rpx 0}.lin-uploader-preview-image{display:block;width:160rpx;height:160rpx;overflow:hidden}.lin-uploader-preview-delete{position:absolute;top:0;right:0;width:28rpx;height:28rpx;background-color:rgba(0,0,0,.7);border-radius:0 0 0 24rpx}.lin-uploader-preview-delete-icon{position:absolute;top:0;right:0;z-index:1;display:flex;flex-direction:row;align-items:center;justify-content:center;width:28rpx;height:28rpx;transform:scale(.6)}.lin-uploader-mask{position:absolute;top:0;right:0;bottom:0;left:0;display:flex;flex-direction:column;align-items:center;justify-content:center;color:#fff;background-color:rgba(50,50,51,.88)}.lin-uploader-message{padding:0 8rpx;margin-top:12rpx;font-size:24rpx;line-height:28rpx;color:#fff}.lin-uploader-file{display:flex;flex-direction:column;align-items:center;justify-content:center;background-color:#f7f8fa}.lin-uploader-file-name{box-sizing:border-box;width:100%;padding:0 8rpx;margin-top:16rpx;font-size:24rpx;color:#646566;text-align:center}.lin-uploader-upload{box-sizing:border-box;display:flex;flex-direction:column;align-items:center;justify-content:center;width:80px;height:80px;margin:0 8px 8px 0;background-color:#f7f8fa}.lin-uploader-uploadText{margin-top:8px;font-size:24rpx;color:#969799}.lin-uploader-slot:empty{display:none}.lin-uploader-slot:not(:empty)+.lin-uploader-upload{display:none!important}.lin-uploader-upload-disabled{opacity:.5}